{
  "name": "DateMarks",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "dev": "set NEXT_PUBLIC_APP_ENV=development && next dev -p 80",
    "dev_s": "set NEXT_PUBLIC_APP_ENV=development && next dev -p 443 --experimental-https",
    "build": "set NEXT_PUBLIC_APP_ENV=production && next build",
    "start": "set NEXT_PUBLIC_APP_ENV=production && npx serve@latest out",
    "prod": "set NEXT_PUBLIC_APP_ENV=production && npm run build && next start -p 443",
    "server_ci-cd": "next start -p 3200",
    "lint": "next lint",
    "export": "next export"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.69.7",
    "server-only": "^0.0.1",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
